{
  "manifest_version": 3,
  "name": "HabboCity - Highscore Collector",
  "version": "1.1.2",
  "description": "Permet de récupérer la liste des joueurs affichés sur un classement.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://cdn.habbocity.fr/*"],
      "js": ["./scripts/content.js"],
      "all_frames": true
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "imgs/icon-16.png",
      "32": "imgs/icon-32.png",
      "48": "imgs/icon-48.png",
      "128": "imgs/icon-128.png"
    }
  },
  "icons": {
    "16": "imgs/icon-16.png",
    "32": "imgs/icon-32.png",
    "48": "imgs/icon-48.png",
    "128": "imgs/icon-128.png"
  }
}
